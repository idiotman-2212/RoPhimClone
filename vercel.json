{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ophim1.com/:path*"
    },
    {
      "source": "/((?!assets|favicon|api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=300, stale-while-revalidate=600" }
      ]
    }
  ]
}
